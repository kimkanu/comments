{
  "extends": ["next", "plugin:tailwindcss/recommended"],
  "rules": {
    "import/prefer-default-export": "off",
    "no-console": "warn",
    "no-var": "error",
    "no-html-link-for-pages": "off",
    "@next/next/no-img-element": "off",
    "tailwindcss/no-custom-classname": ["warn", {
      "whitelist": ["gradient-mask-.*"]
    }]
  }
}
